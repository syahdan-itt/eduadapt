                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Material details",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Material"
                                }
                            }
                        }
                    }
                }
            },
            "put": {
                "tags": [
                    "Materials"
                ],
                "summary": "Update a material by ID",
                "operationId": "ab3ee24b889ae208f872eb9e540de83c",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of the material to update",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "subject_id",
                                    "title",
                                    "content_text",
                                    "difficulty"
                                ],
                                "properties": {
                                    "subject_id": {
                                        "type": "integer"
                                    },
                                    "title": {
                                        "type": "string"
                                    },
                                    "content_text": {
                                        "type": "string"
                                    },
                                    "difficulty": {
                                        "type": "string",
                                        "enum": [
                                            "easy",
                                            "medium",
                                            "hard"
                                        ]
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Material updated successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Material"
                                }
                            }
                        }
                    }
                }
            },
            "delete": {
                "tags": [
                    "Materials"
                ],
                "summary": "Delete a material by ID",
                "operationId": "b9cfea7474e82c257c68c48fa85f104f",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of the material to delete",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "204": {
                        "description": "Material deleted successfully"
                    }
                }
            }
        },
